---
import IdGallery from '@components/gallery/IdGallery.astro';
import { gegenstaendlichData } from '@consts/gegenstaendlichData';

export async function getStaticPaths() {
    return gegenstaendlichData.map((item) => ({
        params: { id: item.id.toString() },
        props: { item },
    }));
}
const { item } = Astro.props;
---

<IdGallery
    title={item.title}
    data={gegenstaendlichData}
    url='/gegenstaendlich'
/>

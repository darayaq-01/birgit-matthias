---
import IdGallery from '@components/gallery/IdGallery.astro';
import { menschenData } from '@consts/__menschenData';

export async function getStaticPaths() {
    return menschenData.map((item) => ({
        params: { id: item.id.toString() },
        props: { item },
    }));
}
const { item } = Astro.props;
---

<IdGallery title={item.title} data={menschenData} url='/menschen' />

---
import IdGallery from '@components/gallery/IdGallery.astro';
import { auftragsarbeitenData } from '@consts/__auftragsarbeitenData';

export async function getStaticPaths() {
    return auftragsarbeitenData.map((item) => ({
        params: { id: item.id.toString() },
        props: { item },
    }));
}
const { item } = Astro.props;
---

<IdGallery
    title={item.title}
    data={auftragsarbeitenData}
    url='/auftragsarbeiten'
/>

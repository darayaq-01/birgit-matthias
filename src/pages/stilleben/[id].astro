---
import IdGallery from '@components/gallery/IdGallery.astro';
import { stillebenData } from '@consts/__stillebenData';

export async function getStaticPaths() {
    return stillebenData.map((item) => ({
        params: { id: item.id.toString() },
        props: { item },
    }));
}
const { item } = Astro.props;
---

<IdGallery title={item.title} data={stillebenData} url='/abstrakte-malerei' />

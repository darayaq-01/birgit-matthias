{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@consts/*": [
        "src/consts/*"
      ],
      "@sections/*": [
        "src/sections/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@constants/*": [
        "src/lib/constants/*"
      ],
      "@helpers/*": [
        "src/lib/helpers/*"
      ],
      "@utils/*": [
        "src/lib/utils/*"
      ],
      "@api/*": [
        "src/lib/api/*"
      ],
    }
  }
}
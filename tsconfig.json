{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "src",
    "strictNullChecks": true,
    "allowJs": true,
    "allowImportingTsExtensions": true,
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@consts/*": [
        "consts/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@sections/*": [
        "sections/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@helpers/*": [
        "helpers/*"
      ],
    }
  }
}